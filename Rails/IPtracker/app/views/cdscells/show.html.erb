<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @cdscell.name %>
</p>

<p>
  <strong>Cdslib:</strong>
  <%= link_to @cdscell.cdslib.name, @cdscell.cdslib %>
</p>

<p>
  <strong>Dimensions</strong>
  x: <%= @cdscell.xsize %> y: <%= @cdscell.ysize %>
</p>
<p>
  <strong>Area</strong>
  <%= @cdscell.area.to_f / (1000.0*1000.0) %> mm^2
</p>
<p>
  <strong>Image:</strong>
  <%= @cdscell.layimg_file_name %>
  <%= image_tag @cdscell.layimg_file_name %>
</p>

<%= link_to 'Edit', edit_cdscell_path(@cdscell) %> |
<%= link_to 'Back', cdscells_path %> |
<%= link_to "Delete", @cdscell, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: @cdscell.name %>