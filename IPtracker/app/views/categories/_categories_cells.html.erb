

<button class='btn-primary' id="expandList"  >Expand All  </button>
<button class="btn"         id="collapseList">Collapse All</button>


<div id="listContainer">
	<ul id="expList">
		<% @categories.each do |category| %>
		<li>
			<%= link_to category.name, category %>
			
			<ul>
	  	  		<% category.cdscells.each do |cdscell| %>
		  			<li> <%= link_to "#{cdscell.cdslib.name} #{cdscell.name}", cdscell %> </li>
		  		<% end %>
	  		</ul>
		</li>  
		<% end %>
		<li>
			Uncategorized
			<ul>
				<% @uncategorized.each do |cdscell| %>
					<li> <%= link_to "#{cdscell.cdslib.name} #{cdscell.name}", cdscell %> </li>
				<% end %>
			<ul>
		</li>
	</ul>
</div>

<%= javascript_include_tag "expandable_list" %>
